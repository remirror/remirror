{
  "name": "support",
  "version": "0.0.0",
  "private": true,
  "description": "The support directory for the remirror monorepo",
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/support",
  "repository": {
    "type": "git",
    "url": "https://github.com/remirror/remirror.git",
    "directory": "support"
  },
  "main": "index.js",
  "dependencies": {
    "@remirror/core-helpers": "4.0.0",
    "diffable-html": "^5.0.0"
  },
  "@remirror": {
    "tsconfigs": {
      "src": false,
      "./": {
        "compilerOptions": {
          "types": [
            "node"
          ],
          "allowJs": true,
          "noEmit": true
        },
        "exclude": [
          "templates"
        ],
        "include": [
          "./*.js",
          "jest",
          "scripts"
        ]
      }
    }
  }
}
