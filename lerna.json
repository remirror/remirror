{
  "publish": {},
  "packages": ["@remirror/*", "support/dependencies", "docs", "packages/*"],
  "command": {
    "publish": {
      "message": "chore(release): publish %s",
      "registry": "https://registry.npmjs.org/",
      "npmClient": "npm",
      "signGitCommit": true,
      "signGitTag": true,
      "allowBranch": ["master"]
    }
  },
  "version": "0.0.1-alpha.3",
  "npmClient": "yarn",
  "registry": "https://registry.npmjs.org/",
  "useWorkspaces": true,
  "ignoreChanges": [
    "**/__fixtures__/**",
    "**/__tests__/**",
    "**/__mocks__/**",
    "**/__stories__/**",
    "/docs/**",
    "**/*.md",
    "**/*.mdx",
    "/support/**",
    "/e2e/**"
  ]
}
