{
  "name": "@remirror/react",
  "version": "1.0.36",
  "description": "Hooks and components for consuming `remirror` with your fave framework `React`.",
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__react",
  "repository": {
    "type": "git",
    "url": "https://github.com/remirror/remirror.git",
    "directory": "packages/remirror__react"
  },
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-react.esm.js",
      "require": "./dist/remirror-react.cjs.js",
      "browser": "./dist/remirror-react.browser.esm.js",
      "types": "./dist/remirror-react.cjs.d.ts",
      "default": "./dist/remirror-react.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "./dist/remirror-react.cjs.js",
  "module": "./dist/remirror-react.esm.js",
  "browser": {
    "./dist/remirror-react.cjs.js": "./dist/remirror-react.browser.cjs.js",
    "./dist/remirror-react.esm.js": "./dist/remirror-react.browser.esm.js"
  },
  "types": "./dist/remirror-react.cjs.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@remirror/extension-placeholder": "^1.0.23",
    "@remirror/extension-positioner": "^1.2.4",
    "@remirror/extension-react-component": "^1.1.11",
    "@remirror/extension-react-ssr": "^1.0.23",
    "@remirror/extension-react-tables": "^1.0.36",
    "@remirror/preset-react": "^1.0.25",
    "@remirror/react-components": "^1.0.33",
    "@remirror/react-core": "^1.2.0",
    "@remirror/react-hooks": "^1.0.33",
    "@remirror/react-renderer": "^1.0.23",
    "@remirror/react-ssr": "^1.0.23",
    "@remirror/react-utils": "^1.0.6"
  },
  "devDependencies": {
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "peerDependencies": {
    "@types/react": "^16.14.0 || ^17 || ^18",
    "@types/react-dom": "^16.9.0 || ^17 || ^18",
    "react": "^16.14.0 || ^17 || ^18",
    "react-dom": "^16.14.0 || ^17 || ^18"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts"
    ]
  },
  "rn:dev": "src/index.ts"
}
