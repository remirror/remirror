{
  "name": "storybook-react",
  "version": "0.0.0",
  "private": true,
  "description": "The storybook configuration for the remirror project.",
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/storybook-react",
  "repository": "https://github.com/remirror/remirror/tree/HEAD/packages/storybook-react",
  "exports": {
    ".": {
      "require": "./dist/storybook-react.cjs.js"
    },
    "./package.json": "./package.json",
    "./storybook-react-main": {
      "require": "./storybook-react-main/dist/storybook-react-storybook-react-main.cjs.js"
    },
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/storybook-react.cjs.js",
  "files": [
    "dist",
    "storybook-react-main"
  ],
  "scripts": {
    "build": "build-storybook -c ./",
    "e2e": "E2E_SERVER=storybook-react pnpm -w e2e",
    "e2e:watch": "E2E_SERVER=storybook-react pnpm -w e2e:watch",
    "start": "start-storybook -p 6006 -c ./"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@emotion/css": "^11.1.3",
    "@remirror/core": "1.0.0",
    "@remirror/dev": "1.0.0",
    "@remirror/extension-react-tables": "1.0.0",
    "@remirror/messages": "1.0.0",
    "@remirror/pm": "1.0.0",
    "@remirror/react": "1.0.0",
    "@remirror/styles": "1.0.0",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.2.8",
    "@storybook/cli": "^6.2.8",
    "@storybook/components": "^6.2.8",
    "@storybook/core": "^6.2.8",
    "@storybook/react": "^6.2.8",
    "@storybook/theming": "^6.2.8",
    "@types/codemirror": "^5.60.0",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/refractor": "^3.0.1",
    "@types/webpack": "^5.28.0",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.2",
    "codemirror": "^5.62.0",
    "create-context-state": "1.0.0",
    "globby": "^11.0.4",
    "is-ci": "^3.0.0",
    "postcss": "^7.0.34",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "reakit": "^1.3.7",
    "refractor": "^3.3.1",
    "remirror": "1.0.0",
    "scripts": "0.0.1"
  },
  "devDependencies": {
    "@types/node": "^15.12.4"
  },
  "@remirror": {
    "tsconfigs": {
      "src": {
        "compilerOptions": {
          "allowJs": true
        }
      },
      "./": {
        "include": [
          "src",
          ".babelrc.js",
          "main.ts"
        ]
      },
      "__stories__": {
        "compilerOptions": {
          "types": [
            "node"
          ]
        }
      }
    }
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "storybook-react-main.ts"
    ]
  }
}
