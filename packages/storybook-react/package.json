{
  "name": "storybook-react",
  "version": "0.0.0",
  "private": true,
  "description": "The storybook configuration for the remirror project.",
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/storybook-react",
  "repository": {
    "type": "git",
    "url": "https://github.com/remirror/remirror.git",
    "directory": "packages/storybook-react"
  },
  "exports": {
    ".": {
      "require": "./dist/storybook-react.cjs.js"
    },
    "./package.json": "./package.json",
    "./storybook-react-main": {
      "require": "./storybook-react-main/dist/storybook-react-storybook-react-main.cjs.js"
    },
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/storybook-react.cjs.js",
  "files": [
    "dist",
    "storybook-react-main"
  ],
  "scripts": {
    "build": "build-storybook -c ./",
    "e2e": "E2E_SERVER=storybook-react pnpm -w e2e",
    "e2e:watch": "E2E_SERVER=storybook-react pnpm -w e2e:watch",
    "start": "start-storybook -p 6006 -c ./"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@emotion/css": "^11.1.3",
    "@joeattardi/emoji-button": "^4.6.0",
    "@popperjs/core": "^2.9.2",
    "@remirror/core": "^1.3.0",
    "@remirror/dev": "^1.0.15",
    "@remirror/extension-file": "^0.3.3",
    "@remirror/extension-mention": "^1.0.10",
    "@remirror/extension-mention-atom": "^1.0.11",
    "@remirror/extension-react-tables": "^1.0.15",
    "@remirror/messages": "^1.0.5",
    "@remirror/pm": "^1.0.6",
    "@remirror/react": "^1.0.15",
    "@remirror/react-editors": "^0.1.26",
    "@remirror/styles": "^1.1.2",
    "@storybook/addon-controls": "^6.3.12",
    "@storybook/addons": "^6.3.4",
    "@storybook/cli": "^6.3.4",
    "@storybook/components": "^6.3.4",
    "@storybook/core": "^6.3.4",
    "@storybook/react": "^6.3.4",
    "@storybook/theming": "^6.3.4",
    "@svgmoji/blob": "^3.1.0",
    "@types/codemirror": "^5.60.2",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/refractor": "^3.0.2",
    "@types/webpack": "^5.28.0",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "codemirror": "^5.62.0",
    "create-context-state": "^1.0.1",
    "globby": "^11.0.4",
    "is-ci": "^3.0.0",
    "postcss": "^8.3.5",
    "postcss-nested": "^5.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "reakit": "^1.3.8",
    "refractor": "^3.3.1",
    "remirror": "1.0.39",
    "scripts": "0.0.1",
    "svgmoji": "^3.2.0",
    "y-webrtc": "^10.2.0",
    "yjs": "^13.5.11"
  },
  "devDependencies": {
    "@types/node": "^16.3.3"
  },
  "@remirror": {
    "tsconfigs": {
      "src": {
        "compilerOptions": {
          "allowJs": true
        }
      },
      "./": {
        "include": [
          "src",
          ".babelrc.js",
          "main.ts",
          "stories"
        ]
      }
    }
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "storybook-react-main.ts"
    ]
  }
}
