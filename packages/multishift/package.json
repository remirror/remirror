{
  "name": "multishift",
  "version": "2.0.9",
  "description": "A repurposed downshift for internal use within remirror",
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/multishift",
  "repository": {
    "type": "git",
    "url": "https://github.com/remirror/remirror.git",
    "directory": "packages/multishift"
  },
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist-types/index.d.ts",
      "import": "./dist/multishift.js",
      "require": "./dist/multishift.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/multishift.cjs",
  "module": "./dist/multishift.js",
  "types": "./dist-types/index.d.ts",
  "files": [
    "dist",
    "dist-types"
  ],
  "dependencies": {
    "@babel/runtime": "^7.22.3",
    "@remirror/core-helpers": "^3.0.0",
    "@remirror/core-types": "^2.0.6",
    "@seznam/compose-react-refs": "^1.0.6",
    "a11y-status": "^2.0.1",
    "compute-scroll-into-view": "^1.0.20",
    "tiny-warning": "^1.0.3",
    "w3c-keyname": "^2.2.7"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.0",
    "react": "^18.2.0"
  },
  "peerDependencies": {
    "@types/react": "^16.14.0 || ^17 || ^18",
    "react": "^16.14.0 || ^17 || ^18"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/multishift.d.ts",
        "import": "./dist/multishift.js",
        "require": "./dist/multishift.cjs"
      },
      "./package.json": "./package.json"
    },
    "main": "./dist/multishift.cjs",
    "module": "./dist/multishift.js",
    "type": "module",
    "types": "./dist/multishift.d.ts"
  },
  "@remirror": {
    "sizeLimit": "20 KB"
  }
