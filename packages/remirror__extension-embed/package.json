{
  "name": "@remirror/extension-embed",
  "version": "1.1.23",
  "description": "Add embedded content into your editor",
  "keywords": [
    "remirror",
    "preset",
    "text-editor"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__extension-embed",
  "repository": {
    "type": "git",
    "url": "https://github.com/remirror/remirror.git",
    "directory": "packages/remirror__extension-embed"
  },
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-extension-embed.esm.js",
      "require": "./dist/remirror-extension-embed.cjs.js",
      "browser": "./dist/remirror-extension-embed.browser.esm.js",
      "types": "./dist/remirror-extension-embed.cjs.d.ts",
      "default": "./dist/remirror-extension-embed.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "./dist/remirror-extension-embed.cjs.js",
  "module": "./dist/remirror-extension-embed.esm.js",
  "browser": {
    "./dist/remirror-extension-embed.cjs.js": "./dist/remirror-extension-embed.browser.cjs.js",
    "./dist/remirror-extension-embed.esm.js": "./dist/remirror-extension-embed.browser.esm.js"
  },
  "types": "./dist/remirror-extension-embed.cjs.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@remirror/core": "^1.4.1",
    "@remirror/messages": "^1.0.6",
    "@types/querystringify": "^2.0.0",
    "prosemirror-resizable-view": "^1.1.11",
    "querystringify": "^2.2.0"
  },
  "devDependencies": {
    "@remirror/pm": "^1.0.14"
  },
  "peerDependencies": {
    "@remirror/pm": "^1.0.11"
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "sizeLimit": "20 KB"
  },
  "rn:dev": "src/index.ts"
}
