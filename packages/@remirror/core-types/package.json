{
  "name": "@remirror/core-types",
  "version": "1.0.0-next.60",
  "description": "Provides the core TS types for use throughout the remirror codebase",
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/@remirror/core-types",
  "repository": "https://github.com/remirror/remirror/tree/HEAD/packages/@remirror/core-types",
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-core-types.esm.js",
      "require": "./dist/remirror-core-types.cjs.js",
      "browser": "./dist/remirror-core-types.browser.esm.js",
      "types": "./dist/remirror-core-types.cjs.d.ts",
      "default": "./dist/remirror-core-types.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/remirror-core-types.cjs.js",
  "module": "dist/remirror-core-types.esm.js",
  "browser": {
    "./dist/remirror-core-types.cjs.js": "./dist/remirror-core-types.browser.cjs.js",
    "./dist/remirror-core-types.esm.js": "./dist/remirror-core-types.browser.esm.js"
  },
  "types": "dist/remirror-core-types.cjs.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@remirror/core-constants": "1.0.0-next.60",
    "type-fest": "^0.20.2"
  },
  "devDependencies": {
    "@remirror/pm": "1.0.0-next.60"
  },
  "peerDependencies": {
    "@remirror/pm": "1.0.0-next.60"
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "sizeLimit": "0 KB"
  }
}
