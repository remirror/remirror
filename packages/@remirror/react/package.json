{
  "name": "@remirror/react",
  "version": "1.0.0-next.60",
  "description": "react utilities for remirror",
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/@remirror/react",
  "repository": "https://github.com/remirror/remirror/tree/HEAD/packages/@remirror/react",
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-react.esm.js",
      "require": "./dist/remirror-react.cjs.js",
      "browser": "./dist/remirror-react.browser.esm.js",
      "types": "./dist/remirror-react.cjs.d.ts",
      "default": "./dist/remirror-react.esm.js"
    },
    "./package.json": "./package.json",
    "./renderers": {
      "import": "./renderers/dist/remirror-react-renderers.esm.js",
      "require": "./renderers/dist/remirror-react-renderers.cjs.js",
      "browser": "./renderers/dist/remirror-react.browser.esm.js",
      "types": "./renderers/dist/remirror-react-renderers.cjs.d.ts",
      "default": "./renderers/dist/remirror-react-renderers.esm.js"
    },
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/remirror-react.cjs.js",
  "module": "dist/remirror-react.esm.js",
  "browser": {
    "./dist/remirror-react.cjs.js": "./dist/remirror-react.browser.cjs.js",
    "./dist/remirror-react.esm.js": "./dist/remirror-react.browser.esm.js"
  },
  "types": "dist/remirror-react.cjs.d.ts",
  "files": [
    "dist",
    "renderers"
  ],
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@remirror/extension-placeholder": "1.0.0-next.60",
    "@remirror/extension-positioner": "1.0.0-next.60",
    "@remirror/extension-react-component": "1.0.0-next.60",
    "@remirror/extension-react-ssr": "1.0.0-next.60",
    "@remirror/i18n": "1.0.0-next.60",
    "@remirror/preset-core": "1.0.0-next.60",
    "@remirror/preset-react": "1.0.0-next.60",
    "@remirror/react-utils": "1.0.0-next.60",
    "@remirror/theme": "1.0.0-next.60",
    "@seznam/compose-react-refs": "^1.0.5",
    "create-context-state": "0.0.0",
    "fast-deep-equal": "^3.1.3",
    "resize-observer-polyfill": "^1.5.1",
    "tiny-warning": "^1.0.3",
    "type-fest": "^0.20.2",
    "use-isomorphic-layout-effect": "^1.1.1",
    "use-previous": "^1.1.0"
  },
  "devDependencies": {
    "@remirror/core": "1.0.0-next.60",
    "@remirror/pm": "1.0.0-next.60",
    "@testing-library/react": "^11.2.3",
    "@testing-library/react-hooks": "^4.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-test-renderer": "^17.0.0",
    "@types/testing-library__react-hooks": "^3.4.1",
    "jest-axe": "^4.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1"
  },
  "peerDependencies": {
    "@remirror/core": "1.0.0-next.60",
    "@remirror/pm": "1.0.0-next.60",
    "@types/react": "^16.14.0 || ^17",
    "@types/react-dom": "^16.9.0 || ^17",
    "react": "^16.14.0 || ^17",
    "react-dom": "^16.14.0 || ^17"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "meta": {
    "sizeLimit": "30 KB"
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "renderers/index.ts"
    ]
  }
}
