{
  "name": "@remirror/extension-collaboration",
  "version": "1.0.0-next.60",
  "description": "Add collaboration to your remirror editor.",
  "keywords": [
    "remirror",
    "remirror-extension",
    "collaboration",
    "editor"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/@remirror/extension-collaboration",
  "repository": "https://github.com/remirror/remirror/tree/HEAD/packages/@remirror/extension-collaboration",
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-extension-collaboration.esm.js",
      "require": "./dist/remirror-extension-collaboration.cjs.js",
      "browser": "./dist/remirror-extension-collaboration.browser.esm.js",
      "types": "./dist/remirror-extension-collaboration.cjs.d.ts",
      "default": "./dist/remirror-extension-collaboration.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/remirror-extension-collaboration.cjs.js",
  "module": "dist/remirror-extension-collaboration.esm.js",
  "browser": {
    "./dist/remirror-extension-collaboration.cjs.js": "./dist/remirror-extension-collaboration.browser.cjs.js",
    "./dist/remirror-extension-collaboration.esm.js": "./dist/remirror-extension-collaboration.browser.esm.js"
  },
  "types": "dist/remirror-extension-collaboration.cjs.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@remirror/messages": "0.0.0",
    "@types/prosemirror-collab": "^1.1.1"
  },
  "devDependencies": {
    "@remirror/core": "1.0.0-next.60",
    "@remirror/pm": "1.0.0-next.60",
    "prosemirror-collab": "^1.2.2"
  },
  "peerDependencies": {
    "@remirror/core": "1.0.0-next.60",
    "@remirror/pm": "1.0.0-next.60",
    "prosemirror-collab": "^1.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "sizeLimit": "5 KB"
  }
}
