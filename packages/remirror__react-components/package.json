{
  "name": "@remirror/react-components",
  "version": "1.0.28",
  "description": "Components to begin your remirror journey",
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__react-components",
  "repository": {
    "type": "git",
    "url": "https://github.com/remirror/remirror.git",
    "directory": "packages/remirror__react-components"
  },
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-react-components.esm.js",
      "require": "./dist/remirror-react-components.cjs.js",
      "browser": "./dist/remirror-react-components.browser.esm.js",
      "types": "./dist/remirror-react-components.cjs.d.ts",
      "default": "./dist/remirror-react-components.esm.js"
    },
    "./all-icons": {
      "import": "./all-icons/dist/remirror-react-components-all-icons.esm.js",
      "require": "./all-icons/dist/remirror-react-components-all-icons.cjs.js",
      "browser": "./all-icons/dist/remirror-react-components-all-icons.browser.esm.js",
      "default": "./all-icons/dist/remirror-react-components-all-icons.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "./dist/remirror-react-components.cjs.js",
  "module": "./dist/remirror-react-components.esm.js",
  "browser": {
    "./dist/remirror-react-components.cjs.js": "./dist/remirror-react-components.browser.cjs.js",
    "./dist/remirror-react-components.esm.js": "./dist/remirror-react-components.browser.esm.js"
  },
  "types": "./dist/remirror-react-components.cjs.d.ts",
  "files": [
    "all-icons",
    "style",
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@lingui/core": "^3.10.4",
    "@popperjs/core": "^2.9.2",
    "@remirror/core": "^1.4.1",
    "@remirror/extension-positioner": "^1.2.2",
    "@remirror/extension-text-color": "^1.0.21",
    "@remirror/i18n": "^1.0.8",
    "@remirror/icons": "^1.0.7",
    "@remirror/messages": "^1.0.6",
    "@remirror/react-core": "^1.1.1",
    "@remirror/react-hooks": "^1.0.28",
    "@remirror/react-utils": "^1.0.6",
    "@remirror/theme": "^1.2.1",
    "@seznam/compose-react-refs": "^1.0.6",
    "@types/react-color": "^3.0.5",
    "create-context-state": "^1.0.1",
    "match-sorter": "^6.3.0",
    "multishift": "^1.0.6",
    "react-color": "^2.19.3",
    "react-popper": "^2.2.5",
    "reakit": "^1.3.8",
    "reakit-system-palette": "^0.14.1",
    "reakit-utils": "^0.15.1",
    "use-isomorphic-layout-effect": "^1.1.1",
    "use-previous": "^1.1.0"
  },
  "devDependencies": {
    "@remirror/pm": "^1.0.14",
    "@remirror/react": "^1.0.29",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "@remirror/pm": "^1.0.10",
    "@types/react": "^16.14.0 || ^17 || ^18",
    "@types/react-dom": "^16.9.0 || ^17 || ^18",
    "react": "^16.14.0 || ^17 || ^18",
    "react-dom": "^16.14.0 || ^17 || ^18"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "sizeLimit": "200 KB"
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "all-icons.ts"
    ]
  },
  "rn:dev": "src/index.ts"
}
