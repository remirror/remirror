{
  "name": "@remirror/extension-yjs",
  "version": "2.0.1",
  "description": "Realtime collaboration with yjs",
  "keywords": [
    "remirror",
    "extension",
    "yjs",
    "realtime",
    "collaboration"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__extension-yjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/remirror/remirror.git",
    "directory": "packages/remirror__extension-yjs"
  },
  "license": "MIT",
  "contributors": [
    "Benjie Gillam <code@benjiegillam.com>",
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-extension-yjs.esm.js",
      "require": "./dist/remirror-extension-yjs.cjs.js",
      "browser": "./dist/remirror-extension-yjs.browser.esm.js",
      "types": "./dist/remirror-extension-yjs.cjs.d.ts",
      "default": "./dist/remirror-extension-yjs.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/remirror-extension-yjs.cjs.js",
  "module": "dist/remirror-extension-yjs.esm.js",
  "browser": {
    "./dist/remirror-extension-yjs.cjs.js": "./dist/remirror-extension-yjs.browser.cjs.js",
    "./dist/remirror-extension-yjs.esm.js": "./dist/remirror-extension-yjs.browser.esm.js"
  },
  "types": "./dist/remirror-extension-yjs.cjs.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@remirror/core": "^1.4.4",
    "@remirror/messages": "^1.0.6",
    "y-prosemirror": "^1.0.19",
    "y-protocols": "^1.0.5"
  },
  "devDependencies": {
    "@remirror/pm": "^1.0.19",
    "y-webrtc": "^10.2.2",
    "yjs": "^13.5.23"
  },
  "peerDependencies": {
    "@remirror/pm": "^1.0.18",
    "yjs": "^13.4.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "sizeLimit": "115 KB"
  },
  "rn:dev": "src/index.ts"
}
