{
  "name": "@remirror/messages",
  "version": "1.0.0",
  "description": "Internalization strings which are used throughout the remirror core codebase.",
  "keywords": [
    "remirror"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__messages",
  "repository": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__messages",
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-messages.esm.js",
      "require": "./dist/remirror-messages.cjs.js",
      "browser": "./dist/remirror-messages.browser.esm.js",
      "types": "./dist/remirror-messages.cjs.d.ts",
      "default": "./dist/remirror-messages.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/remirror-messages.cjs.js",
  "module": "dist/remirror-messages.esm.js",
  "browser": {
    "./dist/remirror-messages.cjs.js": "./dist/remirror-messages.browser.cjs.js",
    "./dist/remirror-messages.esm.js": "./dist/remirror-messages.browser.esm.js"
  },
  "types": "dist/remirror-messages.cjs.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@lingui/core": "^3.10.4",
    "@remirror/core-helpers": "1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "sizeLimit": "50 KB"
  },
  "rn:dev": "src/index.ts"
}
