{
  "name": "@remirror/extension-annotation",
  "version": "1.1.7",
  "description": "This extension allows to annotate the content in your editor",
  "keywords": [
    "remirror",
    "extension"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__extension-annotation",
  "repository": {
    "type": "git",
    "url": "https://github.com/remirror/remirror.git",
    "directory": "packages/remirror__extension-annotation"
  },
  "license": "MIT",
  "contributors": [
    "Ronny Roeller <ronny@nextapp.co>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-extension-annotation.esm.js",
      "require": "./dist/remirror-extension-annotation.cjs.js",
      "browser": "./dist/remirror-extension-annotation.browser.esm.js",
      "types": "./dist/remirror-extension-annotation.cjs.d.ts",
      "default": "./dist/remirror-extension-annotation.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/remirror-extension-annotation.cjs.js",
  "module": "dist/remirror-extension-annotation.esm.js",
  "browser": {
    "./dist/remirror-extension-annotation.cjs.js": "./dist/remirror-extension-annotation.browser.cjs.js",
    "./dist/remirror-extension-annotation.esm.js": "./dist/remirror-extension-annotation.browser.esm.js"
  },
  "types": "dist/remirror-extension-annotation.cjs.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@remirror/core": "^1.3.2",
    "@remirror/extension-positioner": "^1.1.11",
    "@remirror/messages": "^1.0.5"
  },
  "devDependencies": {
    "@remirror/pm": "^1.0.9"
  },
  "peerDependencies": {
    "@remirror/pm": "^1.0.8"
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "sizeLimit": "5 KB"
  },
  "rn:dev": "src/index.ts"
}
