{
  "name": "@remirror/extension-epic-mode",
  "version": "1.0.0",
  "description": "Epic (power) mode extension for the remirror wysiwyg editor.",
  "keywords": [
    "remirror",
    "remirror-extension",
    "power mode",
    "editor effects",
    "epic typing",
    "epic mode",
    "editor"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__extension-epic-mode",
  "repository": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__extension-epic-mode",
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-extension-epic-mode.esm.js",
      "require": "./dist/remirror-extension-epic-mode.cjs.js",
      "browser": "./dist/remirror-extension-epic-mode.browser.esm.js",
      "types": "./dist/remirror-extension-epic-mode.cjs.d.ts",
      "default": "./dist/remirror-extension-epic-mode.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/remirror-extension-epic-mode.cjs.js",
  "module": "dist/remirror-extension-epic-mode.esm.js",
  "browser": {
    "./dist/remirror-extension-epic-mode.cjs.js": "./dist/remirror-extension-epic-mode.browser.cjs.js",
    "./dist/remirror-extension-epic-mode.esm.js": "./dist/remirror-extension-epic-mode.browser.esm.js"
  },
  "types": "dist/remirror-extension-epic-mode.cjs.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@remirror/core": "1.0.0",
    "@remirror/messages": "1.0.0"
  },
  "devDependencies": {
    "@remirror/pm": "1.0.0"
  },
  "peerDependencies": {
    "@remirror/pm": "1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "sizeLimit": "5 KB"
  },
  "rn:dev": "src/index.ts"
}
