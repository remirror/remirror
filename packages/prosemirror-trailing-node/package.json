{
  "name": "prosemirror-trailing-node",
  "version": "2.0.0-beta.0",
  "description": "A trailing node plugin for the prosemirror editor.",
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/prosemirror-trailing-node",
  "repository": {
    "type": "git",
    "url": "https://github.com/remirror/remirror.git",
    "directory": "packages/prosemirror-trailing-node"
  },
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/prosemirror-trailing-node.esm.js",
      "require": "./dist/prosemirror-trailing-node.cjs.js",
      "browser": "./dist/prosemirror-trailing-node.browser.esm.js",
      "types": "./dist/prosemirror-trailing-node.cjs.d.ts",
      "default": "./dist/prosemirror-trailing-node.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/prosemirror-trailing-node.cjs.js",
  "module": "dist/prosemirror-trailing-node.esm.js",
  "browser": {
    "./dist/prosemirror-trailing-node.cjs.js": "./dist/prosemirror-trailing-node.browser.cjs.js",
    "./dist/prosemirror-trailing-node.esm.js": "./dist/prosemirror-trailing-node.browser.esm.js"
  },
  "types": "./dist/prosemirror-trailing-node.cjs.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@remirror/core-constants": "^2.0.0-beta.0",
    "@remirror/core-helpers": "^2.0.0-beta.0",
    "escape-string-regexp": "^4.0.0"
  },
  "devDependencies": {
    "prosemirror-model": "^1.17.0",
    "prosemirror-state": "^1.4.0",
    "prosemirror-view": "^1.25.0"
  },
  "peerDependencies": {
    "prosemirror-model": "^1",
    "prosemirror-state": "^1",
    "prosemirror-view": "^1"
  },
  "peerDependenciesMeta": {},
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "sizeLimit": "10 KB"
  },
  "rn:dev": "src/index.ts"
}
