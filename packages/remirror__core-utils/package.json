{
  "name": "@remirror/core-utils",
  "version": "2.0.0-beta.0",
  "description": "Core utilities for dealing with the dom and prosemirror within remirror",
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__core-utils",
  "repository": {
    "type": "git",
    "url": "https://github.com/remirror/remirror.git",
    "directory": "packages/remirror__core-utils"
  },
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-core-utils.esm.js",
      "require": "./dist/remirror-core-utils.cjs.js",
      "browser": "./dist/remirror-core-utils.browser.esm.js",
      "types": "./dist/remirror-core-utils.cjs.d.ts",
      "default": "./dist/remirror-core-utils.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/remirror-core-utils.cjs.js",
  "module": "dist/remirror-core-utils.esm.js",
  "browser": {
    "./dist/remirror-core-utils.cjs.js": "./dist/remirror-core-utils.browser.cjs.js",
    "./dist/remirror-core-utils.esm.js": "./dist/remirror-core-utils.browser.esm.js"
  },
  "types": "./dist/remirror-core-utils.cjs.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@remirror/core-constants": "^2.0.0-beta.0",
    "@remirror/core-helpers": "^2.0.0-beta.0",
    "@remirror/core-types": "^2.0.0-beta.0",
    "@remirror/messages": "^2.0.0-beta.0",
    "@types/min-document": "^2.19.0",
    "css-in-js-utils": "^3.1.0",
    "min-document": "^2.19.0",
    "parenthesis": "^3.1.8"
  },
  "devDependencies": {
    "@remirror/pm": "^2.0.0-beta.0",
    "@types/jsdom": "^16.2.13",
    "@types/node": "^16.3.3",
    "domino": "^2.1.6",
    "jsdom": "^17.0.0"
  },
  "peerDependencies": {
    "@remirror/pm": "^2.0.0-beta.0",
    "@types/node": "*",
    "domino": "*",
    "jsdom": "*"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    },
    "domino": {
      "optional": true
    },
    "jsdom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "sizeLimit": "25 KB"
  },
  "rn:dev": "src/index.ts"
}
