{
  "name": "@remirror/preset-formatting",
  "version": "1.0.25",
  "description": "A preset with all the formatting node and mark extensions included.",
  "keywords": [
    "remirror",
    "preset",
    "text-editor"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__preset-formatting",
  "repository": {
    "type": "git",
    "url": "https://github.com/remirror/remirror.git",
    "directory": "packages/remirror__preset-formatting"
  },
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
