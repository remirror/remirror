    "@remirror/pm": "workspace:^"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/remirror-extension-bidi.d.ts",
        "import": "./dist/remirror-extension-bidi.js",
        "require": "./dist/remirror-extension-bidi.cjs"
      },
      "./package.json": "./package.json"
    },
    "main": "./dist/remirror-extension-bidi.cjs",
    "module": "./dist/remirror-extension-bidi.js",
    "type": "module",
    "types": "./dist/remirror-extension-bidi.d.ts"
