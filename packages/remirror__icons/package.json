{
  "name": "@remirror/icons",
  "version": "2.0.0-beta.6",
  "description": "RemixIcons for use in your remirror editor.",
  "keywords": [
    "remirror"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__icons",
  "repository": {
    "type": "git",
    "url": "https://github.com/remirror/remirror.git",
    "directory": "packages/remirror__icons"
  },
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/remirror-icons.js",
      "types": "./dist-types/index.d.ts",
      "default": "./dist/remirror-icons.js"
    },
    "./all": {
      "import": "./all/dist/remirror-icons-all.js",
      "types": "./dist-types/all.d.ts",
      "default": "./all/dist/remirror-icons-all.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/remirror-icons.js",
  "module": "./dist/remirror-icons.js",
  "types": "./dist-types/index.d.ts",
  "files": [
    "all",
    "dist",
    "dist-types"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@remirror/core-helpers": "^2.0.0-beta.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "sizeLimit": "30 KB"
  },
  "preconstruct": {
    "entrypoints": [
      "all.ts",
      "index.ts"
    ]
  }
}
