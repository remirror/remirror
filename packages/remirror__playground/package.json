  "module": "dist/remirror-playground.esm.js",
  "browser": {
    "./dist/remirror-playground.cjs.js": "./dist/remirror-playground.browser.cjs.js",
    "./dist/remirror-playground.esm.js": "./dist/remirror-playground.browser.esm.js"
