    "@types/react-dom": "^16.9.0 || ^17",
    "react": "^16.14.0 || ^17",
    "react-dom": "^16.14.0 || ^17"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "preconstruct": {
    "entrypoints": [
