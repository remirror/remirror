    },
    "./worker.typings": {
      "import": "./worker.typings/dist/remirror-playground-worker.typings.esm.js",
      "require": "./worker.typings/dist/remirror-playground-worker.typings.cjs.js",
      "browser": "./worker.typings/dist/remirror-playground.browser.esm.js",
      "default": "./worker.typings/dist/remirror-playground-worker.typings.esm.js"
    }
  },
