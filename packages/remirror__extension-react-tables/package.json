{
  "name": "@remirror/extension-react-tables",
  "version": "1.0.0",
  "description": "Create tables with nested react components.",
  "keywords": [
    "remirror",
    "preset",
    "react",
    "tables",
    "prosemirror",
    "table editing"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__extension-react-tables",
  "repository": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__extension-react-tables",
  "license": "MIT",
  "contributors": [
    "Ocavue <ocavue@gmail.com>",
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-extension-react-tables.esm.js",
      "require": "./dist/remirror-extension-react-tables.cjs.js",
      "browser": "./dist/remirror-extension-react-tables.browser.esm.js",
      "types": "./dist/remirror-extension-react-tables.cjs.d.ts",
      "default": "./dist/remirror-extension-react-tables.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/remirror-extension-react-tables.cjs.js",
  "module": "dist/remirror-extension-react-tables.esm.js",
  "browser": {
    "./dist/remirror-extension-react-tables.cjs.js": "./dist/remirror-extension-react-tables.browser.cjs.js",
    "./dist/remirror-extension-react-tables.esm.js": "./dist/remirror-extension-react-tables.browser.esm.js"
  },
  "types": "dist/remirror-extension-react-tables.cjs.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@emotion/css": "^11.1.3",
    "@linaria/core": "3.0.0-beta.4",
    "@remirror/core": "1.0.0",
    "@remirror/core-utils": "1.0.0",
    "@remirror/extension-positioner": "1.0.0",
    "@remirror/extension-tables": "1.0.0",
    "@remirror/icons": "1.0.0",
    "@remirror/messages": "1.0.0",
    "@remirror/preset-core": "1.0.0",
    "@remirror/react-components": "1.0.0",
    "@remirror/react-core": "1.0.0",
    "@remirror/react-hooks": "1.0.0",
    "@remirror/theme": "1.0.0",
    "jsx-dom": "^6.4.23"
  },
  "devDependencies": {
    "@remirror/pm": "1.0.0",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "y-webrtc": "^10.2.0",
    "yjs": "^13.5.10"
  },
  "peerDependencies": {
    "@remirror/pm": "1.0.0",
    "react": "^16.14.0 || ^17",
    "react-dom": "^16.14.0 || ^17"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "sizeLimit": "20 KB"
  },
  "rn:dev": "src/index.ts"
}
