    "@types/react-dom": "^18.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "peerDependencies": {
    "@remirror/pm": "workspace:^",
    "@types/react": "^16.14.0 || ^17 || ^18",
    "@types/react-dom": "^16.9.0 || ^17 || ^18",
    "react": "^16.14.0 || ^17 || ^18",
    "react-dom": "^16.14.0 || ^17 || ^18"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/remirror-react-ui.d.ts",
