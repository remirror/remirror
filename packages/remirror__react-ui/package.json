{
  "name": "@remirror/react-ui",
  "version": "1.0.3",
  "description": "A selection of optional react components for your remirror editor, powered by @mui/material",
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__react-ui",
  "repository": {
    "type": "git",
