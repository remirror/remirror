    "directory": "packages/remirror__react-ui"
  },
  "license": "MIT",
  "contributors": [
    "Will Hawker <w.hawker@hotmail.co.uk>"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist-types/index.d.ts",
      "import": "./dist/remirror-react-ui.js",
      "require": "./dist/remirror-react-ui.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/remirror-react-ui.cjs",
  "module": "./dist/remirror-react-ui.js",
  "types": "./dist-types/index.d.ts",
  "files": [
    "dist",
    "dist-types",
    "style"
  ],
  "scripts": {
    "build": "remirror-cli build"
  },
