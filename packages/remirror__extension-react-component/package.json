{
  "name": "@remirror/extension-react-component",
  "version": "1.1.9",
  "description": "Create prosemirror node views from your react components",
  "keywords": [
    "remirror",
    "extension",
    "react"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__extension-react-component",
  "repository": {
    "type": "git",
    "url": "https://github.com/remirror/remirror.git",
    "directory": "packages/remirror__extension-react-component"
  },
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-extension-react-component.esm.js",
      "require": "./dist/remirror-extension-react-component.cjs.js",
      "browser": "./dist/remirror-extension-react-component.browser.esm.js",
      "types": "./dist/remirror-extension-react-component.cjs.d.ts",
      "default": "./dist/remirror-extension-react-component.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "./dist/remirror-extension-react-component.cjs.js",
  "module": "./dist/remirror-extension-react-component.esm.js",
  "browser": {
    "./dist/remirror-extension-react-component.cjs.js": "./dist/remirror-extension-react-component.browser.cjs.js",
    "./dist/remirror-extension-react-component.esm.js": "./dist/remirror-extension-react-component.browser.esm.js"
  },
  "types": "./dist/remirror-extension-react-component.cjs.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@remirror/core": "^1.4.1",
    "@remirror/messages": "^1.0.6",
    "nanoevents": "^5.1.13"
  },
  "devDependencies": {
    "@remirror/pm": "^1.0.14",
    "@remirror/react": "^1.0.29",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "@remirror/pm": "^1.0.10",
    "@types/react": "^16.14.0 || ^17 || ^18",
    "@types/react-dom": "^16.9.0 || ^17 || ^18",
    "react": "^16.14.0 || ^17 || ^18",
    "react-dom": "^16.14.0 || ^17 || ^18"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "sizeLimit": "10 KB"
  },
  "rn:dev": "src/index.ts"
}
