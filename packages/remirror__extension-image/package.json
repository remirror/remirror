{
  "name": "@remirror/extension-image",
  "version": "3.0.2",
  "description": "Add images to your editor.",
  "keywords": [
    "remirror",
    "remirror-extension",
    "images",
    "jpg",
    "png",
    "editor"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__extension-image",
  "repository": {
    "type": "git",
    "url": "https://github.com/remirror/remirror.git",
    "directory": "packages/remirror__extension-image"
  },
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist-types/index.d.ts",
      "import": "./dist/remirror-extension-image.js",
      "require": "./dist/remirror-extension-image.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/remirror-extension-image.cjs",
  "module": "./dist/remirror-extension-image.js",
  "types": "./dist-types/index.d.ts",
  "files": [
    "dist",
    "dist-types"
  ],
  "scripts": {
    "build": "remirror-cli build"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.0",
    "@remirror/core": "workspace:*",
    "@remirror/messages": "workspace:*",
    "@remirror/theme": "workspace:*",
    "prosemirror-resizable-view": "workspace:^"
  },
  "devDependencies": {
    "@remirror/cli": "workspace:*",
    "@remirror/pm": "workspace:*"
  },
  "peerDependencies": {
    "@remirror/pm": "workspace:^"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/remirror-extension-image.d.ts",
        "import": "./dist/remirror-extension-image.js",
        "require": "./dist/remirror-extension-image.cjs"
      },
      "./package.json": "./package.json"
    },
    "main": "./dist/remirror-extension-image.cjs",
    "module": "./dist/remirror-extension-image.js",
    "type": "module",
    "types": "./dist/remirror-extension-image.d.ts"
  },
  "@remirror": {
    "sizeLimit": "20 KB"
  }
}
