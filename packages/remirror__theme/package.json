{
  "name": "@remirror/theme",
  "version": "1.0.0",
  "description": "The configurable theme for the remirror editor.",
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__theme",
  "repository": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__theme",
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-theme.esm.js",
      "require": "./dist/remirror-theme.cjs.js",
      "browser": "./dist/remirror-theme.browser.esm.js",
      "types": "./dist/remirror-theme.cjs.d.ts",
      "default": "./dist/remirror-theme.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/remirror-theme.cjs.js",
  "module": "dist/remirror-theme.esm.js",
  "browser": {
    "./dist/remirror-theme.cjs.js": "./dist/remirror-theme.browser.cjs.js",
    "./dist/remirror-theme.esm.js": "./dist/remirror-theme.browser.esm.js"
  },
  "types": "dist/remirror-theme.cjs.d.ts",
  "files": [
    "style",
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@linaria/core": "3.0.0-beta.4",
    "@remirror/core-types": "1.0.0",
    "case-anything": "^1.1.3",
    "color2k": "^1.2.4",
    "csstype": "^3.0.7"
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "sizeLimit": "10 KB"
  },
  "rn:dev": "src/index.ts"
}
