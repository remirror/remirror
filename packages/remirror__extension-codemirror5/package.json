{
  "name": "@remirror/extension-codemirror5",
  "version": "1.0.18",
  "description": "Add CodeMirror to your editor.",
  "keywords": [
    "remirror",
    "remirror-extension",
    "CodeMirror",
    "editor"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__extension-codemirror5",
  "repository": {
