    "@remirror/core": "^1.4.1",
    "@remirror/messages": "^1.0.6"
  },
  "devDependencies": {
    "@remirror/pm": "^1.0.14",
    "@types/codemirror": "^5.60.2",
    "codemirror": "^5.62.0"
