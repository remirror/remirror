{
  "name": "@remirror/extension-react-ssr",
  "version": "1.0.20",
  "description": "If server side support is important to you, then this is perfect for your remirror editor in react.",
  "keywords": [
    "remirror",
    "prosemirror",
    "extension",
    "ssr",
    "server side",
    "react",
    "text-editor"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__extension-react-ssr",
  "repository": {
    "type": "git",
    "url": "https://github.com/remirror/remirror.git",
    "directory": "packages/remirror__extension-react-ssr"
  },
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-extension-react-ssr.esm.js",
      "require": "./dist/remirror-extension-react-ssr.cjs.js",
      "browser": "./dist/remirror-extension-react-ssr.browser.esm.js",
      "types": "./dist/remirror-extension-react-ssr.cjs.d.ts",
      "default": "./dist/remirror-extension-react-ssr.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "./dist/remirror-extension-react-ssr.cjs.js",
  "module": "./dist/remirror-extension-react-ssr.esm.js",
  "browser": {
    "./dist/remirror-extension-react-ssr.cjs.js": "./dist/remirror-extension-react-ssr.browser.cjs.js",
    "./dist/remirror-extension-react-ssr.esm.js": "./dist/remirror-extension-react-ssr.browser.esm.js"
  },
  "types": "./dist/remirror-extension-react-ssr.cjs.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@remirror/core": "^1.4.1",
    "@remirror/extension-react-component": "^1.1.9",
    "@remirror/messages": "^1.0.6",
    "@remirror/react-utils": "^1.0.6"
  },
  "devDependencies": {
    "@remirror/pm": "^1.0.14",
    "@remirror/react": "^1.0.29",
    "@types/react": "^17.0.14",
    "react": "^17.0.2"
  },
  "peerDependencies": {
    "@remirror/pm": "^1.0.10",
    "@types/react": "^16.14.0 || ^17 || ^18",
    "react": "^16.14.0 || ^17 || ^18"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "sizeLimit": "5 KB"
  },
  "rn:dev": "src/index.ts"
}
