{
  "name": "@remirror/extension-tables",
  "version": "1.0.21",
  "description": "A table a day keeps the doctor away. Remirror and play keeps the bad times at bay.",
  "keywords": [
    "remirror",
    "preset",
    "react",
    "tables",
    "prosemirror",
    "table editing"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__extension-tables",
  "repository": {
    "type": "git",
    "url": "https://github.com/remirror/remirror.git",
    "directory": "packages/remirror__extension-tables"
  },
  "license": "MIT",
  "contributors": [
    "Ocavue <ocavue@gmail.com>",
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-extension-tables.esm.js",
      "require": "./dist/remirror-extension-tables.cjs.js",
      "browser": "./dist/remirror-extension-tables.browser.esm.js",
      "types": "./dist/remirror-extension-tables.cjs.d.ts",
      "default": "./dist/remirror-extension-tables.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "./dist/remirror-extension-tables.cjs.js",
  "module": "./dist/remirror-extension-tables.esm.js",
  "browser": {
    "./dist/remirror-extension-tables.cjs.js": "./dist/remirror-extension-tables.browser.cjs.js",
    "./dist/remirror-extension-tables.esm.js": "./dist/remirror-extension-tables.browser.esm.js"
  },
  "types": "./dist/remirror-extension-tables.cjs.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@remirror/core": "^1.4.1",
    "@remirror/messages": "^1.0.6",
    "@remirror/theme": "^1.2.1"
  },
  "devDependencies": {
    "@remirror/pm": "^1.0.14"
  },
  "peerDependencies": {
    "@remirror/pm": "^1.0.10"
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "sizeLimit": "10 KB"
  },
  "rn:dev": "src/index.ts"
}
