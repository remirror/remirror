{
  "name": "prosemirror-paste-rules",
  "version": "1.0.0",
  "description": "Better handling of pasted content in your prosemirror editor.",
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/prosemirror-paste-rules",
  "repository": "https://github.com/remirror/remirror/tree/HEAD/packages/prosemirror-paste-rules",
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/prosemirror-paste-rules.esm.js",
      "require": "./dist/prosemirror-paste-rules.cjs.js",
      "browser": "./dist/prosemirror-paste-rules.browser.esm.js",
      "types": "./dist/prosemirror-paste-rules.cjs.d.ts",
      "default": "./dist/prosemirror-paste-rules.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/prosemirror-paste-rules.cjs.js",
  "module": "dist/prosemirror-paste-rules.esm.js",
  "browser": {
    "./dist/prosemirror-paste-rules.cjs.js": "./dist/prosemirror-paste-rules.browser.cjs.js",
    "./dist/prosemirror-paste-rules.esm.js": "./dist/prosemirror-paste-rules.browser.esm.js"
  },
  "types": "dist/prosemirror-paste-rules.cjs.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@remirror/core-constants": "1.0.0",
    "@remirror/core-helpers": "1.0.0",
    "escape-string-regexp": "^4.0.0"
  },
  "devDependencies": {
    "@types/prosemirror-keymap": "^1.0.4",
    "@types/prosemirror-model": "^1.13.0",
    "@types/prosemirror-state": "^1.2.6",
    "@types/prosemirror-view": "^1.17.1",
    "prosemirror-model": "^1.14.2",
    "prosemirror-state": "^1.3.4",
    "prosemirror-view": "^1.18.7"
  },
  "peerDependencies": {
    "@types/prosemirror-model": "^1",
    "@types/prosemirror-state": "^1",
    "@types/prosemirror-view": "^1",
    "prosemirror-model": "^1",
    "prosemirror-state": "^1",
    "prosemirror-view": "^1"
  },
  "peerDependenciesMeta": {
    "@types/prosemirror-model": {
      "optional": true
    },
    "@types/prosemirror-state": {
      "optional": true
    },
    "@types/prosemirror-view": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "sizeLimit": "10 KB"
  },
  "rn:dev": "src/index.ts"
}
