{
  "name": "@remirror/react-renderer",
  "version": "2.0.7",
  "description": "Render the JSON output from your remirror editor as a react component",
  "keywords": [
    "remirror"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__react-renderer",
  "repository": {
    "type": "git",
    "url": "https://github.com/remirror/remirror.git",
    "directory": "packages/remirror__react-renderer"
  },
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/remirror-react-renderer.js",
      "require": "./dist/remirror-react-renderer.cjs",
      "types": "./dist-types/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/remirror-react-renderer.cjs",
  "module": "./dist/remirror-react-renderer.js",
  "types": "./dist-types/index.d.ts",
  "files": [
    "dist",
    "dist-types"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@remirror/core": "^2.0.7"
  },
  "devDependencies": {
    "@types/react": "^18.0.6",
    "react": "^18.0.0"
  },
  "peerDependencies": {
    "@types/react": "^16.14.0 || ^17 || ^18",
    "react": "^16.14.0 || ^17 || ^18"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "sizeLimit": "50 KB"
  }
}
