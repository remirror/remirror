{
  "name": "@remirror/react-renderer",
  "version": "3.0.2",
  "description": "Render the JSON output from your remirror editor as a react component",
  "keywords": [
    "remirror"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__react-renderer",
  "repository": {
    "type": "git",
    "url": "https://github.com/remirror/remirror.git",
    "directory": "packages/remirror__react-renderer"
  },
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist-types/index.d.ts",
      "import": "./dist/remirror-react-renderer.js",
      "require": "./dist/remirror-react-renderer.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/remirror-react-renderer.cjs",
  "module": "./dist/remirror-react-renderer.js",
  "types": "./dist-types/index.d.ts",
  "files": [
    "dist",
    "dist-types"
  ],
  "scripts": {
    "build": "remirror-cli build"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.0",
    "@remirror/core": "workspace:*"
  },
  "devDependencies": {
    "@remirror/cli": "workspace:*",
    "@types/react": "^18.2.0",
    "react": "^18.3.1"
  },
  "peerDependencies": {
    "@types/react": "^16.14.0 || ^17 || ^18",
    "react": "^16.14.0 || ^17 || ^18"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/remirror-react-renderer.d.ts",
        "import": "./dist/remirror-react-renderer.js",
        "require": "./dist/remirror-react-renderer.cjs"
      },
      "./package.json": "./package.json"
    },
    "main": "./dist/remirror-react-renderer.cjs",
    "module": "./dist/remirror-react-renderer.js",
    "type": "module",
    "types": "./dist/remirror-react-renderer.d.ts"
  },
  "@remirror": {
    "sizeLimit": "50 KB"
  }
