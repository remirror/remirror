{
  "name": "@remirror/dom",
  "version": "2.0.0-beta.13",
  "description": "Use remirror directly in the dom.",
  "keywords": [
    "remirror",
    "extension"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__dom",
  "repository": {
    "type": "git",
    "url": "https://github.com/remirror/remirror.git",
    "directory": "packages/remirror__dom"
  },
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/remirror-dom.js",
      "require": "./dist/remirror-dom.cjs",
      "types": "./dist-types/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/remirror-dom.cjs",
  "module": "./dist/remirror-dom.js",
  "types": "./dist-types/index.d.ts",
  "files": [
    "dist",
    "dist-types"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@remirror/core": "^2.0.0-beta.13",
    "@remirror/preset-core": "^2.0.0-beta.13"
  },
  "devDependencies": {
    "@remirror/pm": "^2.0.0-beta.13"
  },
  "peerDependencies": {
    "@remirror/pm": "^2.0.0-beta.13"
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "sizeLimit": "5 KB"
  }
