{
  "name": "@remirror/react-editors",
  "version": "1.0.0-beta.1",
  "description": "A showcase of possible react editors built with remirror.",
  "keywords": [
    "remirror",
    "text-editors",
    "wysiwyg",
    "markdown",
    "social",
    "mention",
    "react",
    "prosemirror"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__react-editors",
  "repository": {
    "type": "git",
    "url": "https://github.com/remirror/remirror.git",
    "directory": "packages/remirror__react-editors"
  },
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/remirror-react-editors.js",
      "types": "./dist-types/index.d.ts",
      "default": "./dist/remirror-react-editors.js"
    },
    "./markdown": {
      "import": "./markdown/dist/remirror-react-editors-markdown.js",
      "types": "./dist-types/markdown/index.d.ts",
      "default": "./markdown/dist/remirror-react-editors-markdown.js"
    },
    "./package.json": "./package.json",
    "./social": {
      "import": "./social/dist/remirror-react-editors-social.js",
      "types": "./dist-types/social/index.d.ts",
      "default": "./social/dist/remirror-react-editors-social.js"
    },
    "./wysiwyg": {
      "import": "./wysiwyg/dist/remirror-react-editors-wysiwyg.js",
      "types": "./dist-types/wysiwyg/index.d.ts",
      "default": "./wysiwyg/dist/remirror-react-editors-wysiwyg.js"
    }
  },
  "main": "./dist/remirror-react-editors.js",
  "module": "./dist/remirror-react-editors.js",
  "types": "./dist-types/index.d.ts",
  "files": [
    "dist",
    "dist-types",
    "markdown",
    "social",
    "wysiwyg"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@remirror/core-helpers": "^2.0.0-beta.1",
    "@remirror/extension-react-tables": "^2.0.0-beta.1",
    "@remirror/pm": "^2.0.0-beta.1",
    "@remirror/react": "^2.0.0-beta.1",
    "@remirror/styles": "^2.0.0-beta.1",
    "@svgmoji/noto": "^3.2.0",
    "@types/refractor": "^3.0.2",
    "create-context-state": "^2.0.0-beta.0",
    "refractor": "^3.3.1",
    "remirror": "2.0.0-beta.1",
    "svgmoji": "^3.2.0"
  },
  "devDependencies": {
    "@emotion/css": "^11.1.3",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "peerDependencies": {
    "@types/react": "^16.14.0 || ^17 || ^18",
    "@types/react-dom": "^16.9.0 || ^17 || ^18",
    "react": "^16.14.0 || ^17 || ^18",
    "react-dom": "^16.14.0 || ^17 || ^18"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "markdown/index.ts",
      "social/index.ts",
      "wysiwyg/index.ts"
    ]
  }
}
