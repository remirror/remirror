{
  "name": "@remirror/react-editors",
  "version": "0.1.64",
  "description": "A showcase of possible react editors built with remirror.",
  "keywords": [
    "remirror",
    "text-editors",
    "wysiwyg",
    "markdown",
    "social",
    "mention",
    "react",
    "prosemirror"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__react-editors",
  "repository": {
    "type": "git",
    "url": "https://github.com/remirror/remirror.git",
    "directory": "packages/remirror__react-editors"
  },
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-react-editors.esm.js",
      "require": "./dist/remirror-react-editors.cjs.js",
      "browser": "./dist/remirror-react-editors.browser.esm.js",
      "types": "./dist/remirror-react-editors.cjs.d.ts",
      "default": "./dist/remirror-react-editors.esm.js"
    },
    "./markdown": {
      "import": "./markdown/dist/remirror-react-editors-markdown.esm.js",
      "require": "./markdown/dist/remirror-react-editors-markdown.cjs.js",
      "browser": "./markdown/dist/remirror-react-editors-markdown.browser.esm.js",
      "default": "./markdown/dist/remirror-react-editors-markdown.esm.js"
    },
    "./package.json": "./package.json",
    "./social": {
      "import": "./social/dist/remirror-react-editors-social.esm.js",
      "require": "./social/dist/remirror-react-editors-social.cjs.js",
      "browser": "./social/dist/remirror-react-editors-social.browser.esm.js",
      "default": "./social/dist/remirror-react-editors-social.esm.js"
    },
    "./types/*": "./dist/declarations/src/*.d.ts",
    "./wysiwyg": {
      "import": "./wysiwyg/dist/remirror-react-editors-wysiwyg.esm.js",
      "require": "./wysiwyg/dist/remirror-react-editors-wysiwyg.cjs.js",
      "browser": "./wysiwyg/dist/remirror-react-editors-wysiwyg.browser.esm.js",
      "default": "./wysiwyg/dist/remirror-react-editors-wysiwyg.esm.js"
    }
  },
  "main": "./dist/remirror-react-editors.cjs.js",
  "module": "./dist/remirror-react-editors.esm.js",
  "browser": {
    "./dist/remirror-react-editors.cjs.js": "./dist/remirror-react-editors.browser.cjs.js",
    "./dist/remirror-react-editors.esm.js": "./dist/remirror-react-editors.browser.esm.js"
  },
  "types": "./dist/remirror-react-editors.cjs.d.ts",
  "files": [
    "dist",
    "markdown",
    "social",
    "wysiwyg"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@remirror/core-helpers": "^1.0.5",
    "@remirror/extension-react-tables": "^1.0.29",
    "@remirror/pm": "^1.0.14",
    "@remirror/react": "^1.0.29",
    "@remirror/styles": "^1.1.3",
    "@svgmoji/noto": "^3.2.0",
    "@types/refractor": "^3.0.2",
    "create-context-state": "^1.0.1",
    "refractor": "^3.3.1",
    "remirror": "1.0.72",
    "svgmoji": "^3.2.0"
  },
  "devDependencies": {
    "@emotion/css": "^11.1.3",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "@types/react": "^16.14.0 || ^17 || ^18",
    "@types/react-dom": "^16.9.0 || ^17 || ^18",
    "react": "^16.14.0 || ^17 || ^18",
    "react-dom": "^16.14.0 || ^17 || ^18"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "markdown/index.ts",
      "social/index.ts",
      "wysiwyg/index.ts"
    ]
  }
}
