{
  "name": "remirror",
  "version": "1.0.7",
  "description": "One editing package to rule them all, one editing package to bind them.",
  "keywords": [
    "prosemirror",
    "react",
    "remirror",
    "wysiwyg",
    "editing",
    "ssr",
    "text editor"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror",
  "repository": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror",
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>",
    "ocavue "
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror.esm.js",
      "require": "./dist/remirror.cjs.js",
      "browser": "./dist/remirror.browser.esm.js",
      "types": "./dist/remirror.cjs.d.ts",
      "default": "./dist/remirror.esm.js"
    },
    "./dom": {
      "import": "./dom/dist/remirror-dom.esm.js",
      "require": "./dom/dist/remirror-dom.cjs.js",
      "browser": "./dom/dist/remirror.browser.esm.js",
      "types": "./dom/dist/remirror-dom.cjs.d.ts",
      "default": "./dom/dist/remirror-dom.esm.js"
    },
    "./extensions": {
      "import": "./extensions/dist/remirror-extensions.esm.js",
      "require": "./extensions/dist/remirror-extensions.cjs.js",
      "browser": "./extensions/dist/remirror.browser.esm.js",
      "types": "./extensions/dist/remirror-extensions.cjs.d.ts",
      "default": "./extensions/dist/remirror-extensions.esm.js"
    },
    "./icons": {
      "import": "./icons/dist/remirror-icons.esm.js",
      "require": "./icons/dist/remirror-icons.cjs.js",
      "browser": "./icons/dist/remirror.browser.esm.js",
      "default": "./icons/dist/remirror-icons.esm.js"
    },
    "./package.json": "./package.json",
    "./playground": {
      "import": "./playground/dist/remirror-playground.esm.js",
      "require": "./playground/dist/remirror-playground.cjs.js",
      "browser": "./playground/dist/remirror.browser.esm.js",
      "types": "./playground/dist/remirror-playground.cjs.d.ts",
      "default": "./playground/dist/remirror-playground.esm.js"
    },
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/remirror.cjs.js",
  "module": "dist/remirror.esm.js",
  "browser": {
    "./dist/remirror.cjs.js": "./dist/remirror.browser.cjs.js",
    "./dist/remirror.esm.js": "./dist/remirror.browser.esm.js"
  },
  "types": "dist/remirror.cjs.d.ts",
  "files": [
    "dist",
    "dom",
    "extensions",
    "icons",
    "playground",
    "styles"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@remirror/core": "^1.0.2",
    "@remirror/core-constants": "^1.0.0",
    "@remirror/core-helpers": "^1.0.1",
    "@remirror/core-types": "^1.0.1",
    "@remirror/core-utils": "^1.1.0",
    "@remirror/dom": "^1.0.1",
    "@remirror/extension-annotation": "^1.0.1",
    "@remirror/extension-bidi": "^1.0.1",
    "@remirror/extension-blockquote": "^1.0.1",
    "@remirror/extension-bold": "^1.0.1",
    "@remirror/extension-callout": "^1.0.1",
    "@remirror/extension-code": "^1.0.1",
    "@remirror/extension-code-block": "^1.0.2",
    "@remirror/extension-codemirror5": "^1.0.1",
    "@remirror/extension-collaboration": "^1.0.1",
    "@remirror/extension-columns": "^1.0.1",
    "@remirror/extension-diff": "^1.0.1",
    "@remirror/extension-doc": "^1.0.2",
    "@remirror/extension-drop-cursor": "^1.0.1",
    "@remirror/extension-embed": "^1.1.0",
    "@remirror/extension-emoji": "^1.0.1",
    "@remirror/extension-epic-mode": "^1.0.1",
    "@remirror/extension-events": "^1.0.1",
    "@remirror/extension-font-family": "^1.0.1",
    "@remirror/extension-font-size": "^1.0.1",
    "@remirror/extension-gap-cursor": "^1.0.1",
    "@remirror/extension-hard-break": "^1.0.1",
    "@remirror/extension-heading": "^1.0.1",
    "@remirror/extension-history": "^1.0.1",
    "@remirror/extension-horizontal-rule": "^1.0.1",
    "@remirror/extension-image": "^1.0.2",
    "@remirror/extension-italic": "^1.0.1",
    "@remirror/extension-link": "^1.0.1",
    "@remirror/extension-list": "^1.0.3",
    "@remirror/extension-markdown": "^1.0.1",
    "@remirror/extension-mention": "^1.0.1",
    "@remirror/extension-mention-atom": "^1.0.1",
    "@remirror/extension-node-formatting": "^1.0.2",
    "@remirror/extension-paragraph": "^1.0.1",
    "@remirror/extension-placeholder": "^1.0.1",
    "@remirror/extension-positioner": "^1.0.1",
    "@remirror/extension-search": "^1.0.1",
    "@remirror/extension-strike": "^1.0.1",
    "@remirror/extension-sub": "^1.0.1",
    "@remirror/extension-sup": "^1.0.1",
    "@remirror/extension-tables": "^1.0.1",
    "@remirror/extension-text": "^1.0.1",
    "@remirror/extension-text-case": "^1.0.1",
    "@remirror/extension-text-color": "^1.0.1",
    "@remirror/extension-text-highlight": "^1.0.1",
    "@remirror/extension-trailing-node": "^1.0.1",
    "@remirror/extension-underline": "^1.0.1",
    "@remirror/extension-whitespace": "^1.0.1",
    "@remirror/extension-yjs": "^1.0.1",
    "@remirror/icons": "^1.0.2",
    "@remirror/preset-core": "^1.0.1",
    "@remirror/preset-formatting": "^1.0.1",
    "@remirror/preset-wysiwyg": "^1.0.3",
    "@remirror/theme": "^1.1.0",
    "@types/codemirror": "^5.60.2",
    "@types/refractor": "^3.0.2",
    "codemirror": "^5.62.0",
    "refractor": "^3.3.1",
    "yjs": "^13.5.11"
  },
  "devDependencies": {
    "@remirror/pm": "^1.0.1",
    "@types/prettier": "^2.3.2",
    "prettier": "^2.3.2"
  },
  "peerDependencies": {
    "@remirror/pm": "^1.0.1",
    "@types/prettier": "^2.1.5",
    "prettier": "^2.2.0"
  },
  "peerDependenciesMeta": {
    "@types/prettier": {
      "optional": true
    },
    "prettier": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {},
  "preconstruct": {
    "entrypoints": [
      "dom.ts",
      "extensions.ts",
      "icons.ts",
      "index.ts",
      "playground.ts"
    ]
  },
  "rn:dev": "src/index.ts"
}
