{
  "name": "@remirror/types",
  "version": "0.1.0",
  "description": "Base types used throughout the remirror packages.",
  "keywords": [
    "remirror"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/remirror__types/types",
  "repository": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__types",
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-types.esm.js",
      "require": "./dist/remirror-types.cjs.js",
      "browser": "./dist/remirror-types.browser.esm.js",
      "types": "./dist/remirror-types.cjs.d.ts",
      "default": "./dist/remirror-types.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/remirror-types.cjs.js",
  "module": "dist/remirror-types.esm.js",
  "browser": {
    "./dist/remirror-types.cjs.js": "./dist/remirror-types.browser.cjs.js",
    "./dist/remirror-types.esm.js": "./dist/remirror-types.browser.esm.js"
  },
  "types": "dist/remirror-types.cjs.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "type-fest": "^1.2.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "sizeLimit": "0 KB"
  }
}
